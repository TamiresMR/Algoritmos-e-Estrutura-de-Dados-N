#include <stdio.h>

int main()
{
    int vet[10], i, j;
    
    for(i = 0; i < 10; i++)
        scanf("%i", &vet[i]);
    
    for(i = 0; i < 10; i++){
        if (vet[i] <= 0)
            vet[i] = 1;
    }
    
    for(i = 0; i < 10; i++){
        printf("X[%i] = %i\n", i, vet[i]);
    }
    return 0;
}

